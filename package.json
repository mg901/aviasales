{
  "name": "aviasales",
  "version": "1.0.0",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "commit": "cross-env git-cz",
    "pretty": "cross-env fixme=fixme prettier './**/**/**.{json,js,ts}' --write"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "import/no-default-export": 2,
      "import/no-self-import": 2,
      "import/no-named-as-default": 2,
      "arrow-body-style": 2,
      "newline-before-return": 2
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "effector": "^20.9.1",
    "effector-react": "^20.6.0",
    "nanoid": "^2.1.8",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^8.0.2",
    "@types/jest": "^24.0.25",
    "@types/nanoid": "^2.1.0",
    "@types/node": "^13.1.2",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/styled-components": "^4.4.1",
    "commitizen": "^4.0.3",
    "cross-env": "^6.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "react-scripts": "3.3.0",
    "typescript": "^3.7.4"
  }
}
